{
  "private": true,
  "name": "sparql",
  "displayName": "SPARQL",
  "description": "Syntax highlighting for SPARQL",
  "version": "0.0.1",
  "publisher": "BrettZamir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brettz9/sparql"
  },
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sparql",
      "aliases": ["SPARQL", "sparql", "rq"],
      "extensions": [".rq",".sparql"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "sparql",
      "scopeName": "source.sparql",
      "path": "./syntaxes/sparql.tmLanguage.json"
    },
    {
      "scopeName": "markdown.sparql.codeblock",
      "path": "./syntaxes/sparql-injection.json",
      "injectTo": ["text.html.markdown"],
      "embeddedLanguages": {
        "meta.embedded.block.sparql": "sparql"
      }
    }]
  }
}
